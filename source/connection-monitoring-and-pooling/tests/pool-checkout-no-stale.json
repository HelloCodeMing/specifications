{
  "version": 1,
  "style": "unit",
  "description": "must destroy and must not check out a stale connection if found while iterating available connections",
  "poolOptions": {
    "minPoolSize": 3
  },
  "operations": [
    {
      "name": "waitForEvent",
      "event": "ConnectionCreated",
      "count": 3
    },
    {
      "name": "clear"
    },
    {
      "name": "checkOut"
    }
  ],
  "events": [
    {
      "type": "ConnectionPoolCreated",
      "address": 42,
      "options": 42
    },
    {
      "type": "ConnectionPoolCleared",
      "address": 42
    },
    {
      "type": "ConnectionCheckOutStarted"
    },
    {
      "type": "ConnectionClosed",
      "connectionId": 42,
      "reason": "stale"
    },
    {
      "type": "ConnectionClosed",
      "connectionId": 42,
      "reason": "stale"
    },
    {
      "type": "ConnectionClosed",
      "connectionId": 42,
      "reason": "stale"
    },
    {
      "type": "ConnectionCheckedOut",
      "connectionId": 42
    }
  ],
  "ignore": [
    "ConnectionReady",
    "ConnectionCreated"
  ]
}
